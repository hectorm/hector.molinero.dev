{
  "hosting": {
    "public": "public",
    "headers": [{
      "source": "**",
      "headers": [{
        "key": "Content-Security-Policy",
        "value": "default-src 'self'; style-src 'self' 'unsafe-inline';"
      }, {
        "key": "X-XSS-Protection",
        "value": "1;mode=block"
      }, {
        "key": "X-Frame-Options",
        "value": "SAMEORIGIN"
      }]
    }, {
      "source": "**/*.@(asc|txt)",
      "headers": [{
        "key": "Content-Type",
        "value": "text/plain; charset=utf-8"
      }]
    }]
  }
}
